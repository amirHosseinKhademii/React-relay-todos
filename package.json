{
  "name": "initial",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "relay": "relay-compiler"
  },
  "relay": {
    "src": "./src/",
    "schema": "./schema.graphql",
    "language": "typescript",
    "eagerEsModules": true,
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ]
  },
  "babel": {
    "plugins": [
      "relay"
    ]
  },
  "dependencies": {
    "graphql-ws": "^5.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-relay": "14.0.0",
    "react-router-dom": "^6.8.1",
    "recoil": "^0.7.6",
    "relay-runtime": "14.0.0",
    "subscriptions-transport-ws": "^0.11.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-relay": "^14.1.3",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "babel-plugin-relay": "^14.1.0",
    "graphql": "^16.6.0",
    "postcss": "^8.4.20",
    "relay-compiler": "^14.1.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-relay": "^2.0.0",
    "vite-tsconfig-paths": "^4.0.3"
  }
}
